{
    "name": "slept-sort",
    "description": "Asynchronously sleep sort",
    "version": "1.0.0",
    "main": "index.js",
    "typings": "dist/index.d.ts",
    "keywords": [
        "sleep sort",
        "sleep-sort.js",
        "slept sort"
    ],
    "repository": "https://github.com/SaltyAom/slept-sort",
    "bugs": "https://github.com/SaltyAom/issues",
    "homepage": "https://github.com/SaltyAom/slept-sort",
    "license": "MIT",
    "scripts": {
        "prebuild": "rm -rf dist",
        "build": "yarn build:js && yarn build:cjs && yarn build:compress && yarn build:es6 && yarn build:esm && yarn build:umd && yarn build:size",
        "build:js": "tsc",
        "build:cjs": "tsc --project tsconfig-cjs.json",
        "build:es6": "tsc --project tsconfig-es6.json",
        "build:esm": "tsc --project tsconfig-esm.json",
        "build:umd": "tsc --project tsconfig-umd.json",
        "build:compress": "terser dist/index.js -o dist/index.js --compress --mangle --wrap sleptSort",
        "build:size": "cd dist && ls -lh",
        "release": "yarn test && yarn build && yarn publish --non-interactive",
        "test": "jest",
        "test:clear": "jest --clearCache"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.9.5",
        "@babel/preset-typescript": "^7.9.0",
        "@babel/types": "^7.9.5",
        "@types/jest": "^25.2.1",
        "jest": "^25.4.0",
        "terser": "^4.6.12",
        "ts-jest": "^25.4.0",
        "typescript": "^3.8.3"
    },
    "dependencies": {}
}
